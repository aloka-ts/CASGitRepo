	<!-- Cache Option -->
	<cache>false</cache> 

	<!-- recursion option -->
	<recursion>true</recursion>

	<!-- Maximum number of recursion in case of non-terminal NAPTR -->
	<max-recursion>5</max-recursion>

	<parallel-query>false</parallel-query>

	<!-- URI scheme -->
	<uri-scheme>sip</uri-scheme> 

	<!-- Asynchronous call Option --> 	
	<async>true</async>

